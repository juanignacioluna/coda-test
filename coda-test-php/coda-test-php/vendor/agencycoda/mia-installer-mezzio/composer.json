{
    "name": "agencycoda/mia-installer-mezzio",
    "description": "The library for Mezzio",
    "version": "0.0.16",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [{
        "name": "Matias Camiletti",
        "homepage": "https://agencycoda.com"
    }],
    "require": {
        "php": "^7.3",
        "agencycoda/mia-core-mezzio": "dev-main",
        "agencycoda/mia-eloquent-mezzio": "dev-main",
        "laminas/laminas-code": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Mia\\Installer\\": "src/"
        }
    },
    "bin": [
        "bin/coda-install"
    ],
    "scripts": {
        "serve": "php -S 0.0.0.0:8080 -t example/",
        "coda-install": "coda-install"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/agencycoda/mia-core-mezzio.git"
        },
        {
            "type": "git",
            "url": "https://github.com/agencycoda/mia-eloquent-mezzio.git"
        },
        {
            "type": "git",
            "url": "https://github.com/agencycoda/mia-auth-mezzio.git"
        }
    ]
}
